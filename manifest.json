{
    "manifest_version": 2,
    "name": "Linkedin Enter Key Supporter",
    "description": "This extension can use Enter key to send message, and left shift + enter is \n",
    "version": "1.0",
    "browser_action": 
    {
        "default_icon": "send.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["main.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ]
}

